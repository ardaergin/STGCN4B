    "ch_device_mid": 64,
    "ch_property_mid": 64,
    "ch_time_mid": 16,
    "ch_outside_mid": 16,
    
    "ch_room_out": 64,
    "ch_device_out": 32,
    "ch_property_out": 32,
    "ch_time_out": 8,
    "ch_outside_out": 8,
    
